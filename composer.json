{
    "name": "phpsure/phpsure",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PHPSure\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPSure\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "mockery/mockery": "1.6.11",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
